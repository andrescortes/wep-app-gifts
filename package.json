{
  "name": "gifts-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:github": "npm run delete:dist && npm run delete:docs && npm run build:href && npm run copy:dist && npm run delete:dist",
    "build:href": "ng build --base-href ./",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "delete:docs": "del-cli -f docs",
    "delete:dist": "del-cli -f dist",
    "copy:dist": "copyfiles dist/bases/* ./docs -f"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.14",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.25",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.8.2"
  }
}
